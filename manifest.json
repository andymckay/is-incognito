{
  "description": "Is the current window in private browsing mode or not?",
  "manifest_version": 2,
  "name": "Is incognito",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"]
  }
}
